{
  "name": "development",
  "displayName": "Development",
  "description": "Development environment configuration",
  "azure": {
    "resourceGroup": "rg-myapp-dev",
    "location": "East US",
    "staticWebApp": {
      "name": "swa-myapp-dev",
      "sku": "Free"
    },
    "keyVault": {
      "name": "kv-myapp-dev",
      "sku": "standard"
    }
  },
  "application": {
    "features": {
      "debugMode": true,
      "analytics": false,
      "monitoring": "basic"
    },
    "security": {
      "requireAuth": false,
      "cors": {
        "enabled": true,
        "origins": ["*"]
      }
    },
    "performance": {
      "caching": false,
      "compression": false
    }
  },
  "build": {
    "optimization": false,
    "sourceMaps": true,
    "minification": false
  },
  "monitoring": {
    "logging": {
      "level": "debug",
      "console": true
    },
    "metrics": {
      "enabled": false
    }
  },
  "deployment": {
    "autoScaling": false,
    "healthCheck": {
      "enabled": true,
      "timeout": 30
    }
  }
}