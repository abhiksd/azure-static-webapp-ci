{
  "name": "staging",
  "displayName": "Staging",
  "description": "Staging environment configuration",
  "azure": {
    "resourceGroup": "rg-myapp-staging",
    "location": "East US",
    "staticWebApp": {
      "name": "swa-myapp-staging",
      "sku": "Standard"
    },
    "keyVault": {
      "name": "kv-myapp-staging",
      "sku": "standard"
    }
  },
  "application": {
    "features": {
      "debugMode": false,
      "analytics": true,
      "monitoring": "enhanced"
    },
    "security": {
      "requireAuth": true,
      "cors": {
        "enabled": true,
        "origins": ["https://staging.myapp.com"]
      }
    },
    "performance": {
      "caching": true,
      "compression": true
    }
  },
  "build": {
    "optimization": true,
    "sourceMaps": true,
    "minification": true
  },
  "monitoring": {
    "logging": {
      "level": "info",
      "console": false
    },
    "metrics": {
      "enabled": true
    }
  },
  "deployment": {
    "autoScaling": true,
    "healthCheck": {
      "enabled": true,
      "timeout": 60
    }
  }
}